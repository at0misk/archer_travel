		$('.multi-button').removeClass('button-show');
		$('.multiDiv').addClass('hide');
		$('.multiDiv').removeClass('block');
	}
	else if($(this).val() == 'roundtrip'){
		if(multi == true){
			$('.append-div').empty();
			multi = false;
		}
		$('.return-form-group').addClass('block');
